      cursor: pointer;
      font-size: 1.3vw;
      opacity: 0.4;
      background: #808080;
      color: #ffffff;
      pointer-events: none;
    }
    .test-ride-modal .test-ride-modal__content .book-test-ride-btn-box.on div {
      background: #d4202b;
      color: #ffffff;
      opacity: 1;
      pointer-events: all;
    }
    .test-ride-modal .test-ride-modal-close-btn {
      position: absolute;
      top: 0px;
      right: 20px;
      font-size: 40px;
      line-height: 1;
      cursor: pointer;
    }
    .test-ride-modal__backdrop {
      width: 100%;
      height: 100%;
      z-index: 5;
      background-color: rgba(51,51,51,.5);
      overflow-y: auto;
      overscroll-behavior: none;
      cursor: pointer;
    }
  /* 预约弹窗-end */

    /* calendly弹窗 */
    .calendly-modal{
      display:none;
      position: fixed;
      top: 0;
      left: 0;
      right:0;
      z-index: 100;
      bottom: 0;
    }
    .calendly-modal .calendly-modal-content {
      width: 90%;
      background: #f6f6f6;
      border-radius: 10px;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
    }
    .calendly-modal .calendly-modal__content{
      position: relative;
    }
    .calendly-modal  .calendly-modal__content .calendly-content-box {
      padding: 40px;
      height: 80vh;
      overflow-y: hidden;
    }
   .calendly-modal .calendly-modal-close-btn {
      position: absolute;
      top: 0px;
      right: 20px;
      font-size: 40px;
      line-height: 1;
      cursor: pointer;
    }

    .calendly-modal__backdrop {
      width: 100%;
      height: 100%;
      z-index: 5;
      background-color: rgba(51,51,51,.5);
      overflow-y: auto;
      overscroll-behavior: none;
      cursor: pointer;
    }
    .hidden-header-important {
      display: none !important;
    }

    .calendly-spinner {
      position: absolute;
      top: 50%;
      left: 0;
      right: 0;
      transform: translateY(-50%);
      text-align: center;
      z-index: -1;
    }
    .calendly-spinner div {
        display: inline-block;
        width: 18px;
        height: 18px;
        background-color: #e1e1e1;
        border-radius: 50%;
        vertical-align: middle;
        animation: calendly-bouncedelay 1.4s infinite ease-in-out;
        animation-fill-mode: both;
      }
    .calendly-spinner .calendly-bounce1 {
      animation-delay: -0.32s;
    }
    .calendly-spinner .calendly-bounce2 {
      animation-delay: -0.16s;
    }
    @keyframes calendly-bouncedelay {
      0%,
      80%,
      100% {
        transform: scale(0.0);
      }
      40% {
        transform: scale(1.0);
      }
    }
    .test-ride-map-wrapper .close-street-view {
        position: absolute;
        z-index: 2;
        top: 20px;
