  function renderSortListHtml(searchObj = {}, lists) {
    var listsArr = lists || dataList || mapMarkerData;
    var _shopList = listsArr.map(function (v, i) {
      return { ...v, distance: calculateDistance(v.long, v.lat, searchObj.lng, searchObj.lat) };
    });
    if (searchObj.lng) {
      _shopList.sort(function (a, b) {
        return a['distance'] - b['distance'];
      });
    }
    var listHtmlArr = [];
    _shopList.forEach((shop, i) => {
      let listItemHtml =
        "<li id='" +
        shop.id +
        "' class='shop-item' data-long='" +
        shop.long +
        "' data-lat='" +
        shop.lat +
