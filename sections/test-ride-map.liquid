    $('.test-ride-map-wrapper .list-wrapper ul').replaceWith("<ul class='shops-box'>" + listHtmlArr.join('') + '</ul>');
  }
  function setAllMap(map) {
    for (var i = 0; i < markers.length; i++) {
      markers[i].setMap(map);
    }
  }
  function clearMarkers() {
    setAllMap(null);
  }
  var currentLat;
  var currentLong;
  if (navigator.geolocation) {
    navigator.geolocation.getCurrentPosition((position) => {
      currentLat = position.coords.latitude;
      currentLong = position.coords.longitude;
      // renderSortListHtml();
    });
  } else {
    alert('Your browser does not support geolocation targeting！');
  }
  $('.test-ride-map-wrapper .get-current-position').click(function () {
    if (currentLat) {
      var current_marker = new google.maps.Marker({
        position: getLatLng(currentLat, currentLong),
        map: map,
        icon: {
          url: `{{ section.settings.current_position_icon | img_url : 'master' }}`,
          scaledSize: new google.maps.Size(30, 30), // scaled size
        },
      });
      map.setCenter(getLatLng(currentLat, currentLong));
      map.setZoom(12);
    } else {
      if (navigator.geolocation) {
        navigator.geolocation.getCurrentPosition((position) => {
          currentLat = position.coords.latitude;
          currentLong = position.coords.longitude;
          map.setCenter(getLatLng(position.coords.latitude, position.coords.longitude));
          map.setZoom(12);
          var current_marker = new google.maps.Marker({
            position: getLatLng(currentLat, currentLong),
            map: map,
            icon: {
              url: `{{ section.settings.current_position_icon | img_url : 'master' }}`,
              scaledSize: new google.maps.Size(30, 30), // scaled size
            },
          });
        });
      } else {
        alert('Your browser does not support geolocation targeting！');
      }
    }
  });
  function geocode(request) {
    if (request && request.address) {
      geocoder
        .geocode(request)
        .then((result) => {
          const { results } = result;
          console.log({ lat: results[0].geometry.location.lat(), lng: results[0].geometry.location.lng() });
          renderSortListHtml({ lat: results[0].geometry.location.lat(), lng: results[0].geometry.location.lng() });
        })
        .catch((e) => {
          alert('Geocode was not successful for the following reason: ' + e);
        });
    }
  }
  function filterAction(_filterKey) {
    dataList = [];
    if (_filterKey == 'testRide') {
      dataList = mapMarkerData.filter((item) => item.is_test_ride == 'true');
    } else if (_filterKey == 'dealer') {
      dataList = mapMarkerData.filter((item) => item.is_dealer == 'true');
    } else if (_filterKey == 'maintenance') {
      dataList = mapMarkerData.filter((item) => item.is_maintenance == 'true');
    } else {
      dataList = mapMarkerData;
    }
    renderSortListHtml({}, dataList);
    clearMarkers();
    markers = [];
    dataList.forEach(function (v, i) {
      var icon = {
        url: v.icon, // url
        scaledSize: new google.maps.Size(27, 35), // scaled size
      };
      var marker = new google.maps.Marker({
        position: getLatLng(v.lat, v.long),
        map: map,
        icon: icon,
      });
      marker.addListener('click', function () {
        //  map.setZoom(14);
        //  map.setCenter(marker.getPosition());
        //  var targetClass = '.test-ride-map-wrapper aside #'+v.shop_id
        //  var targetOffset = $(targetClass).position().top;
        //  $('.test-ride-map-wrapper .list-wrapper ul li').removeClass('on')
        //  $(targetClass).addClass('on')
        //  $('.test-ride-map-wrapper aside').animate({
        //      scrollTop: targetOffset
        // }, 500);
      });
      markers.push(marker);
    });
  }
  $(' .test-ride-map-wrapper aside .location-search-box .search-btn').click(function () {
    geocode({ address: $('.test-ride-map-wrapper aside .location-search-box input').val() });
  });
  $('.test-ride-map-wrapper aside .location-search-box .search-close-btn').click(function () {
    $('.test-ride-map-wrapper aside .location-search-box input').val('');
    $('.filter-store .filter-item').removeClass('on');
    $('.filter-store .filter-item').first().addClass('on');
    filterAction();
  });
  $('.filter-store .filter-item').click(function () {
    $(this).addClass('on');
    $(this).siblings('.filter-store .filter-item').removeClass('on');
    var _filterKey = $(this).attr('key');
    filterAction(_filterKey);
  });

  $(function () {
    var _testRideModalDoc = $('.test-ride-map-wrapper .test-ride-modal');
    $('.test-ride-map-wrapper .test-ride-modal').remove();
    $('body').append(_testRideModalDoc);
    var _calendlyModalDoc = $('.test-ride-map-wrapper .calendly-modal');
    $('.test-ride-map-wrapper .calendly-modal').remove();
    $('body').append(_calendlyModalDoc);
  });
</script>
