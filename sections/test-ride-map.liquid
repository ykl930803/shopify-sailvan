    .test-ride-modal .test-ride-modal__content .products-box .product-box .bike-name{
      font-size: 18px;
      margin-bottom: 10px;
    }
    .test-ride-modal .test-ride-modal__content .products-box .product-box .bike-desc {
      color: #333333B3;
