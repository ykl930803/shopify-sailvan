{% comment %}test ride map in product page{% endcomment %}

<style>
    .gm-fullscreen-control {
      display: none;
    }
    .map-title {
      /* height: 200px;
      line-height: 200px; */
      padding-top: 50px;
      font-size: 56px;
      text-align: center;
      font-weight: 600;
    }
    .test-ride-map-wrapper {
      position: relative;
      /* margin: 0 calc(150 / 1280 * 100vw); */
      box-shadow: 3px 4px 8px 0px
      rgba(25, 28, 31, 0.08),
      -4px -3px 8px 0px
      rgba(25, 28, 31, 0.08);
      border-radius: 4px;
      color: #0f141f;
      display: flex;
      flex-direction: row-reverse;
      /* margin-bottom: calc(80 / 1280 * 100vw); */
    }
    .test-ride-map-wrapper .google-map-box {
      flex: 1;
      position: relative;
    }
    .test-ride-map-wrapper .google-map-box #googleMap {
      width: 100%;
      height:calc(600 / 1280 * 100vw);
      overflow: hidden;
    }
    .test-ride-map-wrapper .filter-store-wrapper {
      position: absolute;
      z-index: 2;
      right: calc(14 / 1280 * 100vw);
      left: calc(14 / 1280 * 100vw);
      top: 0;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store {
      display: grid;
      grid-template-columns: repeat(4,1fr);
      grid-column-gap: 5px;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item {
      display: flex;
      background: #ffffff;
      height: calc(68 / 1280 * 100vw);
      justify-content: center;
      align-items: center;
      position: relative;
      font-family: PolestarUnica77-Medium;
      font-size: calc(18 / 1280 * 100vw);
      transition: all .3s linear;
      color: #0f141f;
      cursor: pointer;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item:after {
      content: '';
      position: absolute;
      height: 2px;
      background: #e4cf1e;
      left:0;
      right:0;
      bottom:0;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item.on:after {
      background: #000000;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item.on {
      {% comment %} background: {{ settings.color_brand_main }}; {% endcomment %}
      background: #d4202b;
      color: #fff;
      height: calc(88 / 1280 * 100vw);
      margin-top: calc(-10 / 1280 * 100vw);
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item img {
      width: 22px;
      margin-right: 5px;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item .img-normal {
      display: none;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item.on .img-normal {
      display: inherit;
    }
    .test-ride-map-wrapper .filter-store-wrapper .filter-store .filter-item.on .img-on {
      display: none;
    }
    .test-ride-map-wrapper aside {
      width:calc(318 / 1280 * 100vw);
      z-index: 2;
      background: #ffffff;
      font-family: PolestarUnica77-light;
      color: #000000;
      height: calc(600 / 1280 * 100vw);
      overflow-y: auto;
      padding: calc(20 / 1280 * 100vw) calc(25 / 1280 * 100vw);
    }
    .test-ride-map-wrapper aside .header{
      font-size: calc(20 / 1280 * 100vw);
      margin-bottom: calc(15 / 1280 * 100vw);
      font-family: PolestarUnica77-Bold;
      font-weight: 600;
    }
    .test-ride-map-wrapper aside .location-search-box {
      display: flex;
      align-items:center;
      margin-bottom: calc(20 / 1280 * 100vw);
    }
    .test-ride-map-wrapper aside .location-search-box input {
      padding: 0 calc(50 / 1280 * 100vw);
      width: 100%;
      height:calc(56 / 1280 * 100vw);
      font-size: max(calc(14 / 1280 * 100vw),14px);
      border-radius: 6px;
      border: solid 1px #0f141f;
      outline: none;
    }
    .test-ride-map-wrapper aside .location-search-box {
      position: relative;
    }
    .test-ride-map-wrapper aside .location-search-box .search-btn {
        cursor:pointer;
        position: absolute;
        left: calc(16 / 1280 * 100vw);
        width:  calc(17 / 1280 * 100vw);
        height:  calc(17 / 1280 * 100vw);
        display: flex;
        align-items: center;
      }
    .test-ride-map-wrapper aside .location-search-box .search-close-btn {
        cursor:pointer;
        position: absolute;
        right:  calc(16 / 1280 * 100vw);
        width:  calc(14 / 1280 * 100vw);
        height:  calc(14 / 1280 * 100vw);
        display: flex;
        align-items: center;
     }
    .test-ride-map-wrapper aside .filter-by-store-box {
      display: flex;
      margin-bottom: 10px;
    }
    .test-ride-map-wrapper aside .filter-by-store-box .filter-name {
      margin-right: 10px;
    }
    .test-ride-map-wrapper aside .filter-by-store-box .filter-content {
      display:flex;
    }
    .test-ride-map-wrapper aside .filter-by-store-box .filter-content .filter-label {
      display: flex;
      margin-right: 12px;
      align-items:center;
    }

    .test-ride-map-wrapper aside .address {
      margin: 0;
    }
    .test-ride-map-wrapper aside ul {
      margin: 0;
      padding: 0;
    }
    .test-ride-map-wrapper aside ul li {
      list-style: none;
    }
    .test-ride-map-wrapper aside ul li {
      border-top: 1px solid #d1d2d3;
      padding : calc(20 / 1280 * 100vw) 0;
      cursor: pointer;
      position: relative;
    }
    .test-ride-map-wrapper aside ul li:hover .right-arrow{
      right: -8px;
    }
    .test-ride-map-wrapper aside ul li .right-arrow {
      position: absolute;
      top: 50%;
      right:0;
      width: calc(9 / 1280 * 100vw);
      height: calc(17 / 1280 * 100vw);
      transition: .5s cubic-bezier(.42,0,.002,1);
    }
    .test-ride-map-wrapper aside ul li .shop-name{
      /* font-size: calc(20 / 1280 * 100vw);
      color: #0f141f;
      font-family: PolestarUnica77-Bold; */
      display: flex;
      justify-content: space-between;
    }
    .test-ride-map-wrapper aside ul li .shop-name-label {
      flex: 1;
      font-size: calc(20 / 1280 * 100vw);
      color: #0f141f;
      font-weight: 600;
    }
    .test-ride-map-wrapper aside ul li .right-box {
      display: flex;
      margin-left: 10px;
      display: none;
    }
    .test-ride-map-wrapper aside ul li .right-box .distance {
      font-size: 12px;
      line-height: 25px;
    }
    .test-ride-map-wrapper aside ul li .shop-name img{
      width: 25px;
      height: 25px;
      margin-left: 5px;
    }
    .test-ride-map-wrapper aside ul li .ordinary-label {
      color: #979898;
      display: flex;
      font-size: calc(14 / 1280 * 100vw);
      max-width: calc(200 / 1280 * 100vw);
    }
    .test-ride-map-wrapper aside ul li .ordinary-label a {
      color: #979898;
      text-decoration: none;
    }
    .test-ride-map-wrapper aside ul li .ordinary-label.address img {
      width: 11px;
      height: 15px;
      margin-right: 8px;
      margin-top: 4px;
    }
    .test-ride-map-wrapper aside ul li .ordinary-label.phone {
      align-items: center;
    }
    .test-ride-map-wrapper aside ul li .ordinary-label.phone img {
      width: 13px;
      height: 13px;
      margin-right: 8px;
    }
    .test-ride-map-wrapper aside ul li .book-btn-wrapper {
     text-align: center;
     margin-top: 10px;
      display: flex;
      justify-content:space-between;
    }
    .test-ride-map-wrapper aside ul li .book-btn-wrapper .book-btn {
      min-width: calc(105 / 1280 * 100vw);
      {% comment %} background-color: {{ settings.color_brand_main }}; {% endcomment %}
      background-color: #d4202b;
      color: #fff;
      display: inline-block;
      height: calc(28 / 1280 * 100vw);
      line-height: calc(28 / 1280 * 100vw);
      padding: 0 10px;
      border-radius: calc(14 / 1280 * 100vw);
      cursor: pointer;
      font-size: calc(16 / 1280 * 100vw);
      font-family: PolestarUnica77-Medium;
    }

    .test-ride-map-wrapper aside ul li .book-btn-wrapper .view-street-btn {
      display: inline-block;
      height: 2.125vw;
      line-height: 2.125vw;
      padding-left: 1vw;
      border-radius: 30px;
      cursor: pointer;
      font-size: 18px;
      text-decoration: underline;
      display: none;
    }

  /*   预约弹窗-start */
