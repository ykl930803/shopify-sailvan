        "<div class='right-box'>" +
        "<div class='distance'>" +
        shop.distance +
        ' miles</div>' +
        '</div>' +
        '</div>';
      if (shop.daily) {
        listItemHtml = listItemHtml + "<div class='ordinary-label daily'>Daily: " + shop.daily + '</div>';
      }
      listItemHtml =
        listItemHtml +
